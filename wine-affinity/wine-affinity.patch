+    if (!(this = calloc(1, sizeof(*this))))
+        return E_OUTOFMEMORY;
+
+    if (FAILED(res = dxcore_factory_init(this))) {
+        WARN("Failed to initialize factory, hr %#lx.\n", res);
+        free(this);

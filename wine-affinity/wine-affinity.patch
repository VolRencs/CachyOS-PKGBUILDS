+    *ppv = NULL;
+
+    if (dxcore_factory_singleton != NULL) {
+        TRACE("Reusing factory %p\n", dxcore_factory_singleton);
+        return IUnknown_QueryInterface(
+            &dxcore_factory_singleton->IDXCoreAdapterFactory_iface, riid, ppv
+        );
+    }

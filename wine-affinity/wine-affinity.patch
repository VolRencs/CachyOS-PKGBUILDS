+    if (IsEqualIID(iid, &IID_IDXCoreAdapterFactory)
+            || IsEqualIID(iid, &IID_IUnknown))
+    {
+        *factory = this;
+        this->lpVtbl->AddRef(this);
+        return S_OK;
+    }
+    return E_NOINTERFACE;

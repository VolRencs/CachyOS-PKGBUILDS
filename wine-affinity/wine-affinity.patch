+    IDXCoreAdapterFactory *iface,
+    uint32_t num_attributes, const GUID *filter_attributes,
+    REFIID riid, void **ppv
+) {
+    struct dxcore_factory *this = impl_from_IDXCoreAdapterFactory(iface);

+    BOOL is_detached;
     HWND hwnd;
 };
 
@@ -97,12 +98,17 @@ static VkResult win32u_vkCreateWin32SurfaceKHR( VkInstance instance, const VkWin
         list_add_tail( &offscreen_surfaces, &surface->entry );
